    "@teranes/utils": "^10.0.0",
    "@teranes/vue-composables": "^15.0.0",
    "flatpickr": "^4.6.13",
    "lucide-vue-next": "^0.452.0",
    "reflect-metadata": "^0.2.2",
    "tippy.js": "^6.3.7",
    "toastify-js": "^1.12.0",
    "vue": "^3.5.12",
    "zoom-vanilla.js": "^2.0.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
